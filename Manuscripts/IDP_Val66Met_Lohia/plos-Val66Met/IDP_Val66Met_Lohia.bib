pages = {e1000241},
pmid = {19096500},
publisher = {Public Library of Science},
title = {{Disordered flanks prevent peptide aggregation.}},
url = {http://www.ploscompbiol.org/article/info:doi/10.1371/journal.pcbi.1000241;jsessionid=9388C0FEEF46ED0C943A08879D69BC33},
volume = {4},
year = {2008}
}
@article{Chen2005,

publisher = {NIH Public Access},
title = {{Correlation of chemical shifts predicted by molecular dynamics simulations for partially disordered proteins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25416617 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4715900},

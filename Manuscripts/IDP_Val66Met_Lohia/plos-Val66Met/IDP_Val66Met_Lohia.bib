issn = {0021-9258},
journal = {J. Biol. Chem.},
month = {oct},
number = {41},
pages = {25090--25102},
title = {{Phosphorylation-induced Conformational Ensemble Switching in an Intrinsically Disordered Cancer/Testis Antigen}},
url = {http://www.jbc.org/content/290/41/25090.full.pdf http://www.jbc.org/lookup/doi/10.1074/jbc.M115.658583},

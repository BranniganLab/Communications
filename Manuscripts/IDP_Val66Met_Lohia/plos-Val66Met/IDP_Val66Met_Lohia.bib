issn = {1932-6203},
journal = {PLoS One},
month = {dec},
number = {12},
pages = {e0189171},
publisher = {Public Library of Science},
title = {{Hidden $\alpha$-helical propensity segments within disordered regions of the transcriptional activator CHOP}},
url = {https://dx.plos.org/10.1371/journal.pone.0189171},

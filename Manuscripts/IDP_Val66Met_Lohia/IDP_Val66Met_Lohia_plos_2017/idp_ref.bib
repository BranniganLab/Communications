doi = {10.1021/jp4115404},
file = {:Users/lohia/Library/Application Support/Mendeley Desktop/Downloaded/Nguyen, Tarus, Derreumaux - 2014 - Familial Alzheimer A2 V Mutation Reduces the Intrinsic Disorder and Completely Changes the Free En(2).pdf:pdf},

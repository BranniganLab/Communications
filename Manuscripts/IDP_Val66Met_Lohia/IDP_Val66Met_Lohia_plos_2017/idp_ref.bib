journal = {Proteins Struct. Funct. Bioinforma.},
keywords = {Interresidue correlations,Intraresidue correlations,Knowledge-based potentials,Ramachandran map,Torsion angle correlations,Triplet},
month = {apr},
number = {4},
pages = {992--998},
pmid = {15146495},
title = {{Relationships between amino acid sequence and backbone torsion angle preferences}},
url = {http://portal.ku.edu.tr/{~}okeskin/PAPERS/torsionalangle-proteins.pdf http://doi.wiley.com/10.1002/prot.20100 http://www.ncbi.nlm.nih.gov/pubmed/15146495},

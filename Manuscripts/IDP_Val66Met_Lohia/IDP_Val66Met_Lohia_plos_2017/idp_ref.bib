keywords = {80 and over,Adult,Aged,Brain-Derived Neurotrophic Factor,Brain-Derived Neurotrophic Factor: genetics,Case-Control Studies,Eating Disorders,Eating Disorders: genetics,Female,Genetic,Genetic Predisposition to Disease,Humans,Male,Mental Disorders,Mental Disorders: genetics,Methionine,Methionine: genetics,Middle Aged,Polymorphism,Schizophrenia,Schizophrenia: genetics,Substance-Related Disorders,Substance-Related Disorders: genetics,Valine,Valine: genetics},
month = {apr},

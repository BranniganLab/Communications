@article{Creamer1992,
abstract = {In recent host-guest studies, the helix-forming tendencies of amino acid residues have been quantified by three groups, each obtaining similar results [Padmanabhan, S., Marqusee, S., Ridgeway, T., Laue, T. M. Science 250, 669-673]. Here, we explore the hypothesis that these measured helix-forming propensities are due primarily to conformational restrictions imposed upon residue side chains by the helix itself. This proposition is tested by calculating the extent to which the bulky helix backbone "freezes out" available degrees of free-dom in helix side chains. Specifically, for a series of apolar residues, the difference in configurational entropy, AS, be-tween each side chain in the unfolded state and in the a-helical state is obtained from a simple Monte Carlo calculation. These computed entropy differences are then compared with the experimentally determined values. Measured and calculated values are found to be in close agreement for naturally occur-ring amino acids and in total disagreement for non-natural amino acids. In the calculation, ASAIa = 0. The rank order of entropy loss for the series of natural apolar side chains under consideration is Ala {\textless} Leu {\textless} Trp {\textless} Met {\textless} Phe {\textless} Ile {\textless} Tyr {\textless} Val. Among these, none favor helix formation; Ala is neutral, and all remaining residues are unfavorable to varying degrees. Thus, applied to side chains, the term "helix prefer-ence" is a misnomer. While side chain-side chain interactions may modulate stability in some instances, our results indicate that the drive to form helices must originate in the backbone, consistent with Pauling's view of four decades ago [Pauling, L., Corey, R. B. {\&} Branson, H. R. (1951) Proc. Nati. Acad. Sci. USA 37, 205-210].},
author = {Creamer, Trevor P and Rose, George D},
file = {:Users/lohia/Library/Application Support/Mendeley Desktop/Downloaded/Creamer, Rose - 1992 - Side-chain entropy opposes a-helix formation but rationalizes experimentally determined helix-forming propensi(2).pdf:pdf},
journal = {Nat. Sci.},
number = {250},
pages = {5937--5941},
title = {{Side-chain entropy opposes a-helix formation but rationalizes experimentally determined helix-forming propensities (a-helix/protein folding/protein engineering)}},
volume = {89},
year = {1992}
}
@article{Abraham2015,

title = {{Membrane-lipid therapy: A historical perspective of membrane-targeted therapies — From lipid bilayer structure to the pathophysiological regulation of cells ☆ Peripheral membrane protein localization and activity Lipids in pathophysiology Membrane lipid c}},
url = {https://ac.els-cdn.com/S0005273617301785/1-s2.0-S0005273617301785-main.pdf?{\_}tid=cabc3336-bdb3-11e7-8cfd-00000aab0f6c{\&}acdnat=1509396735{\_}d3c3183bc01379f37a7ea4aa897c7669 http://ac.els-cdn.com/S0005273617301785/1-s2.0-S0005273617301785-main.pdf?{\_}tid=35fbf2ca},

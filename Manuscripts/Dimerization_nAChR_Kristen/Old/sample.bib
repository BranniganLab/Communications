journal = {The Journal of Experimental Medicine},
number = {6},
pages = {1121--1134},
pmid = {22585736},
title = {{Cannabinoids suppress inflammatory and neuropathic pain by targeting $\alpha$3 glycine receptors}},
url = {http://www.jem.org/lookup/doi/10.1084/jem.20120242},
volume = {209},
year = {2012}

title = {{The effect of general anesthetics on the dynamics of phosphatidylcholine-acetylcholine receptor interactions in reconstituted vesicles}},
url = {https://ac.els-cdn.com/000527369390325T/1-s2.0-000527369390325T-main.pdf?{\_}tid=682aea07-8cfd-4560-bb2e-c955b2ab0d6f{\&}acdnat=1532297522{\_}cf086bbea36dc22c3b6c7e5b144ce63f},

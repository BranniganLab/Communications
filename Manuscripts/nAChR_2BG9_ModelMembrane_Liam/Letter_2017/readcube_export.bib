issn = {2050084X},
journal = {eLife},
keywords = {Cys-loop receptors,allosteric modulation,nanodiscs,site-directed spin labeling},
month = {mar},
pages = {e23886},
pmid = {28262093},
publisher = {eLife Sciences Publications Limited},
title = {{Crystal structure and dynamics of a lipid-induced potential desensitized-state of a pentameric ligand-gated channel}},
url = {https://elifesciences.org/articles/23886},

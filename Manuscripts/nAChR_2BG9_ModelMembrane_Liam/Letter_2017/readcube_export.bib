file = {:Users/liamsharp/Documents/ReadCube Media/Cotman et al-1969-Biochemistry-us.pdf:pdf},
issn = {15204995},
journal = {Biochemistry},
number = {11},
pages = {4606--4612},
pmid = {4311035},
title = {{Lipid Composition of Synaptic Plasma Membranes Isolated from Rat Brain by Zonal Centrifugation}},
volume = {8},
year = {1969}

issn = {0031-9333},
journal = {Physiological Reviews},
number = {3},
pages = {1189--1234},
pmid = {22811427},
title = {{End-Plate Acetylcholine Receptor: Structure, Mechanism, Pharmacology, and Disease}},
url = {http://physrev.physiology.org/cgi/doi/10.1152/physrev.00015.2011},
volume = {92},
year = {2012}
}

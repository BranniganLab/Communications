@article{Mantipragada_Lipid_2003,
  abstract = {The selectivity of lipid−protein interaction for spin-labeled phospholipids and gangliosides in nicotinic acetylcholine receptor-rich membranes from Torpedo marmorata has been studied by ESR spectroscopy. The association constants of the spin-labeled lipids (relative to phosphatidylcholine) at pH 8.0 are in the order cardiolipin (5.1) ≈ stearic acid (4.9) ≈ phosphatidylinositol (4.7) {\textgreater} phosphatidylserine (2.7) {\textgreater} phosphatidylglycerol (1.7) {\textgreater} GD1b ≈ GM1 ≈ GM2 ≈ GM3 ≈ phosphatidylcholine (1.0) {\textgreater} phosphatidylethanolamine (0.5). No selectivity for mono- or disialogangliosides is found over that for phosphatidylcholine. Aminated local anesthetics were found to compete with spin-labeled phosphatidylinositol, but to a much lesser extent with spin-labeled stearic acid, for sites on the intramembranous surface of the protein. The relative association constant of phosphatidylinositol was reduced in the presence of the different local anesthetics to the following extents: tetracaine (55{\%}) {\textgreater} procaine (35{\%}) ≈ benzocaine (30{\%}). For stearic acid, only tetracaine gave an appreciable reduction (30{\%}) in association constant. These displacements represent an intrinsic difference in affinity of the local anesthetics for the lipid−protein interface because the membrane partition coefficients are in the order benzocaine ≫ tetracaine ≈ procaine.},
author = {Mantipragada, S. B. and Horv{\'{a}}th, L. I. and Arias, H. R. and Schwarzmann, G. and Sandhoff, K. and Barrantes, F. J. and Marsh, D.},
doi = {10.1021/bi034485q},
file = {:Users/liamsharp/Documents/ReadCube Media/Mantipragada et al-2003-Biochemistry.pdf:pdf},
isbn = {0006-2960 (Print)},
issn = {00062960},
journal = {Biochemistry},
number = {30},
pages = {9167--9175},
pmid = {12885251},
title = {{Lipid-protein interactions and effect of local anesthetics in acetylcholine receptor-rich membranes from Torpedo marmorata electric organ}},
volume = {42},
year = {2003}
}

file = {:Users/liamsharp/Library/Application Support/Mendeley Desktop/Downloaded/Abadji et al. - 1994 - Lipid-protein interactions and protein dynamics in vesicles containing the nicotinic acetylcholine receptor a stu.pdf:pdf},
journal = {Biochim Biophys Acta},
keywords = {Acetylcholine receptor,EPR,Ethanol,Lipid-protein interaction,Reconstitution},
number = {1},
pages = {25--34},
title = {{Lipid-protein interactions and protein dynamics in vesicles containing the nicotinic acetylcholine receptor: a study with ethanol}},
url = {https://ac.els-cdn.com/0005273694901996/1-s2.0-0005273694901996-main.pdf?{\_}tid=c22f6cff-d73b-4922-bded-cb72939fc370{\&}acdnat=1532297358{\_}7ff5e60b287687df031cf16d1bbfc8d3},

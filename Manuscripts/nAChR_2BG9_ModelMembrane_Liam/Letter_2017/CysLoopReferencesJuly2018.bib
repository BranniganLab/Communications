@Article{Zhu2018,
  author       = {Zhu, Shaotong and Noviello, Colleen M and Teng, Jinfeng and Walsh, Richard M and Kim, Jeong Joo and Hibbs, Ryan E},
  title        = {Structure of a human synaptic GABAA receptor.},

  issn-linking    = {0163-7827},
  issue           = {4},
  keywords        = {Animals; Cell Membrane, metabolism, physiology; Cholesterol, metabolism, physiology; Humans; Lipid Metabolism; Membrane Microdomains, metabolism; Membrane Proteins, metabolism; Protein Binding, physiology; Receptor, Serotonin, 5-HT1A, metabolism; Receptors, G-Protein-Coupled, physiology; Structure-Activity Relationship},
  nlm-id          = {7900832},
  owner           = {NLM},
  pii             = {S0163-7827(06)00016-6},
  pmid            = {16616960},
  pubmodel        = {Print-Electronic},
  pubstatus       = {ppublish},

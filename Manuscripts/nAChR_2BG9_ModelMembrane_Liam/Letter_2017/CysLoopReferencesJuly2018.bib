@Article{Olsen2003,
  author   = {Olsen, Rolf Erik and Dragnes, Bj{\o}rn Tore and Myklebust, Reidar and Ring{\o}, Einar},
  title    = {{Effect of soybean oil and soybean lecithin on intestinal lipid composition and lipid droplet accumulation of rainbow trout, Oncorhynchus mykiss Walbaum}},
  journal  = {Fish Physiology and Biochemistry},

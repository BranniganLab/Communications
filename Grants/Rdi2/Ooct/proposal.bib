isbn = {0006-3495 (Print)},
issn = {00063495},
journal = {Biophysical Journal},
number = {6},
pages = {3709--3721},
pmid = {15189867},
title = {{Tilt angles of transmembrane model peptides in oriented and non-oriented lipid bilayers as determined by2H solid-state NMR}},

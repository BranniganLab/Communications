isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Binding Sites,Cholesterol,Cholesterol: chemistry,Cholesterol: metabolism,Computer Simulation,Hydrogen Bonding,Models, Molecular,Phospholipids,Phospholipids: metabolism,Protein Binding,Protein Structure, Tertiary,Receptors, Nicotinic,Receptors, Nicotinic: chemistry,Receptors, Nicotinic: metabolism,Water,Water: metabolism},
number = {38},
pages = {14418--23},
pmid = {18768796},
title = {{Embedded cholesterol in the nicotinic acetylcholine receptor.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2567199{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {105},

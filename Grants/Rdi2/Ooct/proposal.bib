doi = {10.1104/pp.82.3.807},
file = {:Users/liamsharp/Library/Application Support/Mendeley Desktop/Downloaded/Yoshida, Uemura - 1986 - Lipid Composition of Plasma Membranes and Tonoplasts Isolated from Etiolated Seedlings of Mung Bean (Vigna radi.pdf:pdf},
isbn = {0032-0889 (Print)},
issn = {0032-0889},
journal = {Plant Physiol},
pages = {807--812},
pmid = {16665114},
title = {{Lipid Composition of Plasma Membranes and Tonoplasts Isolated from Etiolated Seedlings of Mung Bean (Vigna radiata L.)1 2}},
